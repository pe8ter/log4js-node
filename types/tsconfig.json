{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "log4js": ["."]
    }
  }
}
